<Window x:Class="FileVisualizor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileVisualizor"
        mc:Ignorable="d"
        Title="File Visualizor" Height="620" Width="620">
    <Canvas Name="canvas"
            HorizontalAlignment="Left"
            Margin="0,0,0,0"
            VerticalAlignment="Top"
            Height="20">
        <Button x:Name="btnPrev" Content="Previous" Canvas.Left="230" Canvas.Top="550" Width="75" Click="btnPrev_Click" />
        <Label x:Name="lblPage" 
               HorizontalAlignment="Center"
               HorizontalContentAlignment="Center"
               Canvas.Top="520" 
               Width="620"/>
        <Button x:Name="btnNext" Content="Next" Canvas.Left="320" Canvas.Top="550" Width="75" Click="btnNext_Click" />
        <Button x:Name="btnGen" Content="Generate" Canvas.Left="525" Canvas.Top="550" Width="75" Click="btnGen_Click" />
        <ComboBox x:Name="cobGen" Canvas.Left="450" Canvas.Top="520" Width="150" />
    </Canvas>
</Window>
